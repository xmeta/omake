OCAMLINCLUDES[] +=
    ../libmojave
    ../util
    ../exec
    ../magic
    ../ast

FILES[] =
    omake_options
    omake_symbol
    omake_state
    omake_node_type
    omake_node_sig
    omake_node
    omake_virtual_id
    omake_install
    omake_ir
